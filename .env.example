# .env.example - Пример файла с переменными окружения
# Скопируйте этот файл как .env и заполните своими значениями

# Настройки сервера
PORT=5000

# Настройки MongoDB
MONGODB_URI=mongodb://localhost:27017/solana_bot_db

# Настройки администратора (используются при первом запуске)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# Настройки Solana
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Настройки Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Настройки бота
MIGRATION_THRESHOLD=98
TARGET_PROFIT=10
PURCHASE_AMOUNT_SOL=0.05
MAX_HOLDING_TIME=3600
CHECK_INTERVAL=5